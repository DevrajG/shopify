<!DOCTYPE html>
<html lang="en" class="cbp-spmenu-push">
  <head>
    <meta name="google-site-verification" content="Oryf2UY9FwvJ07-o9R7S6uHCFsk8A5E_iEKiFlChO_w" />

  <meta charset="utf-8">
  <title>
    {% if template contains "index" and page_title != 'Welcome' %}
      {{ page_title }}
    {% elsif template contains "index" and pages[settings.homepage].title != blank %}
      {{ pages[settings.homepage].title }} | {{ shop.name }}
    {% else %}
      {{ page_title }}
      {% if current_tags %}tagged "{{ current_tags }}"{% endif %}
      {% if current_page != 1 %}Page {{ current_page }}{% endif %}
      | {{ shop.name }}
    {% endif %}
  </title>

  {% include 'page-meta' %}
  <meta name="robots" content="index,follow">
  <meta name="viewport" content="width=device-width, initial-scale=1"> 
  <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">

  {% if collection.previous_product %}
  <link rel="prev" href="{{ collection.previous_product }}">
  {% endif %}
  {% if collection.next_product %}
  <link rel="prev" href="{{ collection.next_product }}">
  {% endif %}

  <!-- Icons -->
  <link rel="shortcut icon" type="image/x-icon" href="{{ 'favicon.png' | asset_url }}">
  <link rel="canonical" href="{{ canonical_url }}" />

  <!-- Styles -->
  {% comment %}{{ '//opensource.keycdn.com/fontawesome/4.6.3/font-awesome.min.css' | stylesheet_tag }}{% endcomment %}
    {{ 'font-awesome.min.css' | asset_url | stylesheet_tag }}
    {{ 'jquery.fullPage.css' | asset_url | stylesheet_tag }}
    {{ 'styles.scss.css' | asset_url | stylesheet_tag }}



    <!-- Scripts -->
    {% comment %}
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
    <script src="https://cdn.optimizely.com/js/3401110080.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.min.js"></script>
    {% endcomment %}

    {{ 'jquery.min.js' | asset_url | script_tag }}
    {{ '3401110080.js' | asset_url | script_tag }}  
    {{ 'jquery.blockUI.js' | asset_url | script_tag }}
    {{ 'tytabs.jquery.min.js' | asset_url | script_tag }}

    {{ 'jquery.fullPage.js' | asset_url | script_tag }}
    {{ 'parallax.min.js' | asset_url | script_tag }}
  
    

  {{ content_for_header }}
  
  {% include 'marketing-crazyegg' %}
<script data-cfasync="false" type="text/javascript" async src="https://cdn.judge.me/shopify.js"></script>
</head>

<body class="{{ template }} landingPage" id="{{ page_title | handle }}">  
  

  <div class="landing_content"> 

  {% unless settings.featured_content_page == blank or template == 'index' %}
      <div class="top-banner">
        {{ pages[settings.featured_content_page].content }}
    </div>
  {% endunless %}

  {{ content_for_layout }}

  </div>

  
  <!-- Scripts -->
  {% if template contains 'customer' %}
  {{ "shopify_common.js" | shopify_asset_url | script_tag }}
  {{ "customer_area.js"  | shopify_asset_url | script_tag }}
  {% endif %}

  {{ 'app.js' | asset_url | script_tag }}

  {% if settings.product_slideshow_animation == 'zoom' %}
    {{ "cloudzoom.js" | asset_url | script_tag }}
  {% endif %}  


  {% include 'newsletter-popup' %}

  {% include 'klaviyo-email-signup' %}  
  {% include 'klaviyo-website-tracking' %}  


  
  {% include 'taboola' %} 
  {{ '//assets.pcrl.co/js/jstracker.min.js'| script_tag }}

  <!-- CRITEO START-->
  {% include 'criteo-tracking' %}
  <!-- CRITEO END-->
  
  {% include 'judgeme_html_miracle' %}
  
  
  {% include 'google-remarketing-tag' %}
  
<!--   <script>
    $(document).ready(function() {
      $('#fullpage').fullpage({
      	navigation: true
      });
    });
  </script> -->
</body>
</html>



