.collections-menu.nav {
  border-bottom: 1px solid $color__bgr-inv;
  border-top: 2px solid white;
  color: $color__bgr-inv;
  height: auto;
  padding: 12px 0 12px 0;
  width: 100%;
  z-index: 800;
  background: #fff;
  font-weight: 500;

  @media (max-width: 767px) {
    position: static !important;
    text-align: initial;
    overflow-x: scroll;
    height: 2rem;
    white-space: nowrap;
  }
  @media (min-width: 768px) {
    position: static;
    > ul {
      display: flex;
      justify-content: space-around;
      width: 64%;
      float: left;
      margin-top: 9px;
      
      @media only screen and (min-width: 768px) and (max-width: 980px){
        display: block;
        width: 100%;
        margin-top: 9px;
      }
      
      li{
        @media only screen and (min-width: 981px) and (max-width: 1024px){
          margin: 0 8px;
          font-size: 13px;
        }
        @media only screen and (min-width: 1025px) and (max-width: 1280px){
          margin: 0 0.4rem;
        }
        
        a{
          position: relative;
          
          @media only screen and (min-width: 981px) and (max-width: 1024px){
            white-space: nowrap;
            font-size: 13px;
          }
          @media only screen and (min-width: 1025px) and (max-width: 1280px){
            white-space: nowrap;
            font-size: 13px;
          }
          
          &:after{
            content: '';
            position: absolute;
            height: 2px;
            width: 100%;
            background: #1C5B93;
            bottom: -4px;
            left: .0;
            opacity: 0;
            transition: all 0.4s;
            -webkit-transition: all 0.4s;
            -moz-transition: all 0.4s;
            -ms-transition: all 0.4s;
          }
          
          &.active{
            &:after{
              opacity: 1;
            }
          }
        }
      }
    }
  }
  
  #sizeFilter{
    width: 33%;
    position: relative;
    float: right;
    top: 0 !important;
    
    @media only screen and (min-width: 768px) and (max-width: 980px){
      width: 100%;
      margin-top: 20px;
    }
    
    ul{
      li{
        @media only screen and (min-width: 981px) and (max-width: 1024px){
          font-size: 12px;
          text-transform: lowercase;
        }
        @media only screen and (min-width: 1025px) and (max-width: 1280px){
          font-size: 13px;
          text-transform: lowercase;
          
        }
        &:last-child{
          display: none;
        }
      }
      li:not(:first-child){
        margin: 0;
        
        a{
          opacity: 0.5;
          height: 32px;
          width: 32px;
          line-height: 36px;
          border: 1px solid transparent;
          border-radius: 100%;
          margin: 0 5px;
          display: block;
          
          @media only screen and (min-width: 981px) and (max-width: 1024px){
            height: 28px;
            width: 28px;
            line-height: 31px;
            margin: 0 1px;
            font-size: 12px;
          }
          @media only screen and (min-width: 1025px) and (max-width: 1280px){
            height: 30px;
            width: 30px;
            line-height: 33px;
            margin: 0;
            font-size: 12px;
          }
        }
        
        &.check{
          a{
            opacity: 1;
            border: 1px solid #1C5B93;
          }
        }
      }
    }
    
    &:before{
      content: '';
      background: rgba(0,0,0,0.2);
      width: 1px;
      position: absolute;
      top: 0;
      left: 16px;
      height: 35px;
      
      @media only screen and (min-width: 768px) and (max-width: 980px){
        content: none;
      }
      @media only screen and (min-width: 981px) and (max-width: 1024px){
        top: 3px;
        left: 3px;
        height: 27px;
      }
      @media only screen and (min-width: 1025px) and (max-width: 1280px){
        left: 0;
      }
    }
  }
}

