/* #Site Styles
================================================== */


/* #Hero
================================================== */
.hero {
  position: relative;
  margin-top:3em;
  margin-bottom:3em;
  @media screen and (max-width:800px) {
    &.flexslider {
      margin-bottom: -1.5rem;
    }
  }
}
.hero:first-child {
  margin-top: 0;
}
.hero:last-child {
  margin-bottom: 0;
}
.hero-caption {
  margin: 0;
  box-sizing: border-box;
  max-width: 50%;
  padding-left: 10%;
  position: absolute; 
  top: 50%;
  -webkit-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
  text-align: center;
  @media screen and (max-width:800px) {
    padding: 0 10px !important;
    -webkit-transform: none;
    -ms-transform: none;
    transform: none;
    position: static;
    max-width: 100%;
    color: inherit !important;
    text-align: center;
    margin-top: 20px;
    margin-bottom: 40px;
  }
}
@media screen and (min-width:801px) {
  .hero-caption.align-right {
    padding-left: 0;
    right: 0;
    padding-right: 10%;
  }
  .hero-caption.align-center {
    padding-left: 0;
    left: 50%;
    -webkit-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
  }
}
.hero-caption .headline {
  @include heading;
  color: inherit;
}
.hero-caption .subtitle {
