/* #Lists
================================================== */
  ul, ol { margin-bottom: 20px; }
  ul { list-style: circle outside; }
  ol { list-style: decimal; }
  ul, ol, ul.square, ul.circle, ul.disc { margin-left: 30px; }
  ul.square { list-style: square outside; }
  ul.circle { list-style: circle outside; }
  ul.disc { list-style: disc outside; }
  ul ul, ul ol,
  ol ol, ol ul { margin: 4px 0 5px 30px; font-size: 90%;  }
  ul ul li, ul ol li,
  ol ol li, ol ul li { margin-bottom: 6px; }
  li { line-height: 18px; margin-bottom: 12px; }
  ul.large li { line-height: 21px; }
  ul.none { list-style: none outside; margin-left: 0;}
  ul.border {
    list-style: none outside;
    line-height: 26px;
  }
  ul.border li {
    border-bottom: 1px solid $color__text-main;
    list-style: none outside none;
    padding: 12px 0;
    margin-bottom: 0;
  }

/* #Menu
================================================== */
.nav {
  text-align:center;
  height: 80px;
  font-size: 14px;
  padding: 30px 0;
  box-sizing: border-box;
  position: relative;
}
.nav .menu {
  position: absolute;
  top: 30px;
}
.nav {
  ul {
    display: block;
    border: none;
    padding: 0;
    margin: 0;
  }
  li {
    display: inline-block;
    position: relative;
    margin: 0 1rem;
    border: 0;
    padding: 0;
    
    .sale{
      color: #e5182c;
      font-weight: 600;
    }
  }
}
#nav{
  .sale{
    a{
      color: #e5182c;
      font-weight: 600;
    }
  }
}
.dropdown {
  opacity: 0.95;
  background-color: $color__txt-inv;
  color: $color__text-main;
  visibility: hidden;
  position: absolute;
  left: -1rem;
  top: 100%;
  z-index: 1005;
  min-width: 100px;
  text-align: left;
}
.dropdown ul {
  padding: 0.5em 0;
}
.dropdown li {
  display: block;
  margin: 0;
  padding: 0 0rem;
}
.dropdown a {
  display: block;
  width: 100%;
  white-space: nowrap;
  padding: 0.5em 0;
}
.menu li:hover > .dropdown {
  visibility: visible;
}


.toggle_menu {
  position: relative;
  overflow: hidden;
  display: none;
  top: -16px;
}

.cbp-spmenu {
	background: $color__txt-inv;
	position: fixed;
	width: 440px;
	height: 100%;
	top: 0;
	z-index:99999;
    overflow-Y: auto;
    li.Label{
      padding:20px;
    }
    ul,li {
      list-style:none;
      padding: 0;
      margin: 0;
      overflow: hidden;
    }
    .continue-item,
    .subtotal-item {
      display: block;
      background-color:$color__bgr-inv;
      color: $color__txt-inv;
    }
    a {
        display: block;
        padding: .4em;
      font-size: 1.5em;
    }
  .sub-menu {
    margin-left: 3em;
  }
}
.cbp-spmenu-left {
	left: -440px;
}
.cbp-spmenu-right {
	right: -440px;
}
.cbp-spmenu-left.cbp-spmenu-open {
	left: 0px;
}

.cbp-spmenu-right.cbp-spmenu-open {
	right: 0px;
}
.cbp-spmenu-push {
	position: relative;
	left: 0;
}
.cbp-spmenu-push-toright {
	left: 440px;
    overflow:  hidden;
}

.cbp-spmenu-push-toleft {
	left: -440px;
    overflow:  hidden;
}
.cbp-spmenu,
.cbp-spmenu-push {
	transition: all 0.3s ease;
}
.cbp-blocker {
  display: none;
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 999999;
  top: 0;
  left: 0;
  right: 0;
}
.cbp-spmenu-push-toleft,
.cbp-spmenu-push-toright {
  .cbp-blocker {
    display: block;
  }
}
@media screen and (max-width: 549px) {
  .cbp-spmenu {
    width: 80%;
  }
  .cbp-spmenu-left {
      left: -80%;
  }
  .cbp-spmenu-right {
      right: -80%;
  }
  .cbp-spmenu-push-toright {
      left: 80%;
  }

  .cbp-spmenu-push-toleft {
      left: -80%;
  }
}
.continue-item {
  height: 80px;
  line-height: 80px;
}
.continue {
  text-indent: 20px;
  position: relative;
}
.continue:before {
  content: '';
  border-left: 2px solid $color__txt-inv;
  border-top: 2px solid $color__txt-inv;
  margin-bottom: -15px;
  left: 22px;
  display: block;
  width: 7px;
  height: 7px;
  margin-bottom: -5px;
  position: absolute;
  bottom: 50%;
  -webkit-transform: rotate(-45deg);
  -ms-transform: rotate(-45deg);
  transform: rotate(-45deg);
}
.counter {
  float: right;
}

/* #Images
================================================== */
img { 
  max-width: 100%;
  height: auto;
  margin-top: -6px;
}
.zoomImg {
  max-width: inherit !important;
  cursor: pointer;
}
.hero img {
  width: 100%;
  display: block;
}

/* #Buttons
================================================== */
  
a.button, 
button,
input[type="submit"],
input[type="reset"],
input[type="button"], 
.action_button, 
a.action_button, 
input.action_button[type="submit"], 
input.action_button[type="button"] {
  background: $color__txt-act;
  color: $color__txt-inv;
  @include heading-alt;
  font-size: 0.95rem;
  font-weight: 400;
  border: 0;
  padding: 12px 20px;
  text-align: center;
  cursor: pointer;
  display: inline-block;
  transition: all 200ms ease 0s;
  border-radius:0;
  font-smoothing: antialiased;
  box-sizing: border-box;
  -webkit-appearance:none;
  -moz-appearance:none;
  -ms-appearance:none;
  -o-appearance:none;
  appearance:none;
}

a.button:active, 
  button:active,
  input[type="submit"]:active,
  input[type="reset"]:active,
  input[type="button"]:active,
  .action_button:active, 
  input.action_button[type="submit"]:active, 
  input.action_button[type="button"]:active {
    box-shadow: inset 0 2px 4px rgba(0,0,0,.15), 0 1px 2px rgba(0,0,0,.05);
}

.add_to_cart, 
#cart_form .add_to_cart, 
form.product_form input.add_to_cart {
  background-color: $color__text-main;
  width: 100%;
  padding-top: 1.5em;
  padding-bottom: 1.5em;
  margin: 2rem 0;
  &:hover {
    background-color: $color__txt-act;
  }
}

#cart a.action_button.edit_cart {
  width:180px;margin-left: 20px;margin-top: 20px; background-color: $color__txt-alt; color: $color__txt-inv; text-align: center; text-indent: 0;
}

#cart .action_button {
  width:180px;margin-right: 20px; margin-top: 20px; background-color: $color__txt-act; color: $color__txt-inv; text-align: center; text-indent: 0;
}

  
/* #Forms
================================================== */
form { 
  margin-bottom: 20px; }
fieldset { 
    margin-bottom: 20px; }
input[type="text"], 
  input[type="number"],
  input[type="password"],
  input[type="email"],
  textarea, 
  select {
    box-sizing: border-box;
    border: 1,5px solid #949494;
    padding: 10px 8px 10px 12px;
    outline: none;
    border-radius: 0;
    font-size: 15px;
    font-family: inherit;
    font-weight: normal;
    color: #555;
    width: 230px;
    margin: 0;
    max-width: 100%;
    display: block;
    margin-bottom: 20px;
    background: #fff; 
    transition: border-color 0.2s linear, box-shadow 0.2s ease;
    -webkit-appearance: none;
    -moz-appearance: none;
    -ms-appearance: none;
    -o-appearance: none;
    appearance: none;}
select {  
    background: #fff url({{ 'select.png' | asset_url }}) no-repeat right center;
    cursor: pointer;
    -webkit-padding-end: 30px; 
    background-size: 17px 5px; 
}
input[type="number"] {
  padding: 5px;
}
input[type="text"]:hover,
  input[type="number"]:hover,
  input[type="password"]:hover,
  input[type="email"]:hover, 
  textarea:hover,
  select:hover {
    border-color: rgb(138, 137, 138);
    box-shadow: rgba(55, 55, 55, 0) 0px 0px 0px 1px;
}
input[type="text"]:focus,
  input[type="number"]:focus,
  input[type="password"]:focus,
  input[type="email"]:focus, 
  textarea:focus,
  select:focus {
    border-color: rgb(138, 137, 138);
    color: #333;
}
label,
  legend { 
    display: block;
    font-weight: bold;
    margin-bottom: 4px; }
input[type="checkbox"] {
    display: inline; }
label span,
  legend span {
    font-weight: normal;
    font-size: 15px; }
textarea {
    width: 100%;
  }

.quantity_label {
  display: inline;
  font-size: smaller;
}  
.remove_item a {
  font-size: smaller;
  color: {{ settings.regular_color }};
}
input.quantity {
  width: 48px;
  display: inline;
  margin-bottom: 0;
  padding: 8px 5px;
}
.cart_items p {
  margin-bottom: 5px; 
}
li.cart_item a {
  text-indent: 0 !important;
  font-size: small;
}
.cart_image {
  padding: 10px 20px 0 20px;
  width: 100px;
  text-align: center;
  float: left;
}
#get-rates-submit {
  margin-top: -1px;
}
#customer_login {
  margin-bottom: 2px;
}

#contact_email,
#k_id_email,
.sign_up{
  margin: 0 0 1em;
  box-sizing: border-box;
  width: 200px;
  line-height: 21px;
  margin-bottom: 5px;
}
#k_id_email,
.footer .newsletter input[type="email"] {
  max-width: 200px;
  border-radius: 0;
  border: 1px solid #1C5B93;
  padding: 10px 10px 10px 10px;
  font-size: 16px;
  margin-bottom: 5px;
}

.klaviyo_submit_button,
.footer .newsletter button.sign_up {
  width: 230px;
  padding: 10px;
  border-radius: 0;
  border: none;
  margin-top: 5px;
}
.klaviyo_field_group,
.klaviyo_form_actions {
  float:left;
}
.success_message{
  @include heading-alt;
}

/* #Search
================================================== */
#search { 
  display: none;
  width: 100%;
  //height: 100%;
  left: 0;
  top: 29px;
  z-index: 900;
  position: absolute;
  text-align: center;
  overflow: hidden;
  box-sizing: border-box;
  backface-visibility: hidden;
  padding: 20px;
  background: #1c3b57;
  //background: #003658;

  form {
  z-index: 950;
    margin: 0px;
    input {
      width: 100%;
      box-sizing: border-box;
      border: none;
      border-radius: none;
      padding: 0;
      font-size: 14px;
      background: inherit;
      color: #fff !important;
      margin: 0px;
      
      &::-webkit-input-placeholder { /* Chrome/Opera/Safari */
        color: #fff;
      }
      &::-moz-placeholder { /* Firefox 19+ */
        color: #fff;
      }
      &:-ms-input-placeholder { /* IE 10+ */
        color: #fff;
      }
      &:-moz-placeholder { /* Firefox 18- */
        color: #fff;
      }
    }
  }
  
  .search-close {
    font-size: 22px;
    line-height: 1em;
    cursor: pointer;
    position: absolute;
    top: 6px;
    right: 9px;
    color: #fff;
    padding: 10px;
    font-weight: 100;
  }
}

.mobile-search {
  background: inherit;
  width: 100%;
  padding: 10px;
  position: relative;
  top: 0;
  z-index: 2;
  box-sizing: border-box;
  form {
    margin: 0;
  }
  input[type=text] {
    border: none;
    border-radius: none;
    font: inherit;
    line-height: 30px;
    outline: none;
    display: block;
    width: 100%;
    height: 30px;
    margin: 0;
    padding: 30px 10px;
    background: #003658;
    color: #fff !important;
    box-sizing: border-box;
    
    &::-webkit-input-placeholder { /* Chrome/Opera/Safari */
      color: #fff;
    }
    &::-moz-placeholder { /* Firefox 19+ */
      color: #fff;
    }
    &:-ms-input-placeholder { /* IE 10+ */
      color: #fff;
    }
    &:-moz-placeholder { /* Firefox 18- */
      color: #fff;
    }
  }
}


