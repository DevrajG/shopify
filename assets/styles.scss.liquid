/* #Reset & Basics
================================================== */
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video {
  margin: 0;
  padding: 0;
  border: 0;
  font: inherit;
}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {
  display: block;
}
body {
  line-height: 1;
}
ol, ul {
    list-style: none;
}
blockquote, q {
  quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
  content: '';
  content: none;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}

/* #Basic Styles
================================================== */
$color__text-main: #1C5B93; //dark navy
$color__text-heading: #1C5B93; //navy
$color__txt-act: #1C5B93; //dark navy
$color__txt-inv: #fff; //white
$color__txt-alt: #d2d5d8; //grey
$color__bgr-main: #fff; //white
$color__bgr-inv: #1C5B93; //dark blue
$color__bgr-alt: #c9c8ce; //grey

body { 
  position: relative;
  background-color: $color__bgr-inv;
  -webkit-font-smoothing: antialiased;
  font-smoothing: antialiased;
  color: $color__text-main;
}

/* #Typography
================================================== */

$font__sans:  'gt_walsheim_pro_regular';

@mixin regular-text {
  font: normal normal 1rem/1.25em $font__sans;
  text-transform: none;
}
@mixin heading {
  font: normal 700 1.75rem/1.25em $font__sans;
  text-transform: none;
}
@mixin heading-alt {
  font: normal 500 1.25rem/1.25em $font__sans;
  text-transform: none;
}
:root {
  font-size: 16px;
}
html,
body {
  @include regular-text;
  font-family: 'gt_walsheim_pro_regular','Helvetica Neue', 'arial';
}
h1, h2, h3, h4, h5, h6, .collection_title, .headline, p.subtitle { 
  @include heading;
  color: $color__text-heading;
  margin:1em .25em;
  display: block;
  text-rendering: optimizeLegibility;
}
h1,h2,h3 {
  font-size: 2em;
}
h4,h5,h6 {
  font-size: inherit;
  margin: .5em 0;
}
p {
  margin-bottom: 1em;
}
p img { margin: 0;}
em, i { 
  //font-style: italic; 
  font-family: 'gt_walsheim_pro_regular_oblique';
}
strong, b { font-weight: bold; }
small { font-size: 90%; }

/*  Blockquotes  */
blockquote,
blockquote p,
.quote {
  font-size: inherit; 
  font-weight: normal;
  text-align: center;
  margin: 3em auto;
}
blockquote cite { display: block; font-size: 12px; color: #555; }
blockquote cite:before { content: "\2014 \0020"; }
blockquote cite a, blockquote cite a:visited, blockquote cite a:visited { color: #555; }
hr { border:none; clear: both; margin: 12px 0; height: 0; }

/* #Links
================================================== */
a, a:visited { 
  color: inherit;
  text-decoration: none; 
  text-transform: lowercase;
  transition: color .1s linear;
}


