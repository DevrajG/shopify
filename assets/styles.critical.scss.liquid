html,
body,
div,
span,
h3,
p,
a,
img,
ul,
li,
form {
  margin: 0;
  padding: 0;
  border: 0;
  font: inherit;
}

body {
  line-height: 1;
  position: relative;
  background-color: #1c3b57;
  -webkit-font-smoothing: antialiased;
  font-smoothing: antialiased;
  color: #16191e;
}
:root {
  font-size: 16px;
}
ul {
  list-style: none;
}
html,
body {
  font: normal normal 1rem / 1.25em "Atlas Grotesk Web", sans-serif;
  text-transform: none;
}

h3,
.headline {
  font: normal 700 1.75rem / 1.25em "Atlas Grotesk Web", sans-serif;
  text-transform: none;
  color: #2c3d57;
  margin: 1em 0;
  display: block;
  text-rendering: optimizeLegibility;
}

h3 {
  font-size: 2em;
}

p {
  margin-bottom: 1em;
}

a {
  color: inherit;
  text-decoration: none;
}

ul {
  margin-bottom: 20px;
  list-style: circle outside;
  margin-left: 30px;
}

ul ul {
  margin: 4px 0 5px 30px;
  font-size: 90%;
}

ul ul li {
  margin-bottom: 6px;
}

li {
  line-height: 18px;
  margin-bottom: 12px;
}

.nav {
  text-align: center;
  height: 80px;
  font-size: 14px;
  padding: 30px 0;
  box-sizing: border-box;
  position: relative;
}

.nav .menu {
  position: absolute;
  top: 30px;
}

.nav ul {
  display: block;
  border: none;
  padding: 0;
  margin: 0;
}

.nav li {
  display: inline-block;
  position: relative;
  margin: 0 1rem;
  border: 0;
  padding: 0;
}

.dropdown {
  opacity: 0.95;
  background-color: #fff;
  color: #16191e;
  visibility: hidden;
  position: absolute;
  left: -1rem;
  top: 100%;
  z-index: 1005;
  min-width: 200px;
  text-align: left;
}

.dropdown ul {
  padding: 0.5em 0;
}

.dropdown li {
  display: block;
  margin: 0;
  padding: 0 1rem;
}

.dropdown a {
  display: block;
  width: 100%;
  white-space: nowrap;
  padding: 0.5em 0;
}

.cbp-spmenu {
  background: #fff;
  position: fixed;
  width: 440px;
  height: 100%;
  top: 0;
  z-index: 0;
  overflow-Y: auto;
}

.cbp-spmenu ul,
.cbp-spmenu li {
  list-style: none;
  padding: 0;
  margin: 0;
  overflow: hidden;
}

.cbp-spmenu .continue-item {
  display: block;
  background-color: #1c3b57;
  color: #fff;
}

.cbp-spmenu a {
  display: block;
  padding: 1em;
}

.cbp-spmenu .sub-menu {
  margin-left: 3em;
}

.cbp-spmenu-left {
  left: -440px;
}

.cbp-spmenu-right {
  right: -440px;
}

.cbp-spmenu-push {
  position: relative;
  left: 0;
}


.cbp-blocker {
  display: none;
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 999999;
  top: 0;
  left: 0;
  right: 0;
}

@media screen and (max-width: 549px) {
  .cbp-spmenu {
    width: 80%;
  }

  .cbp-spmenu-left {
    left: -80%;
  }

  .cbp-spmenu-right {
    right: -80%;
  }
}

.continue-item {
  height: 80px;
  line-height: 80px;
}

.continue {
  text-indent: 20px;
  position: relative;
}

.continue:before {
  content: '';
  border-left: 2px solid #fff;
  border-top: 2px solid #fff;
  margin-bottom: -15px;
  left: 22px;
  display: block;
  width: 7px;
  height: 7px;
  margin-bottom: -5px;
  position: absolute;
  bottom: 50%;
  -webkit-transform: rotate(-45deg);
  -ms-transform: rotate(-45deg);
  transform: rotate(-45deg);
}

img {
  max-width: 100%;
  height: auto;
}

.hero img {
  width: 100%;
  display: block;
}

button,
.action_button,
a.action_button {
  background: #16191e;
  color: #fff;
  font: normal 500 1.25rem / 1.25em "Atlas Grotesk Web", sans-serif;
  text-transform: uppercase;
  font-size: 0.95rem;
  font-weight: 400;
  border: 0;
  padding: 12px 20px;
  text-align: center;
  display: inline-block;
  border-radius: 0;
  font-smoothing: antialiased;
  box-sizing: border-box;
  -webkit-appearance: none;
  -moz-appearance: none;
  -ms-appearance: none;
  -o-appearance: none;
  appearance: none;
}

form {
  margin-bottom: 20px;
}

input[type="text"],
input[type="email"] {
  box-sizing: border-box;
  border: 1, 5px solid #949494;
  padding: 10px 8px 10px 12px;
  outline: none;
  border-radius: 0;
  font-size: 15px;
  font-family: inherit;
  font-weight: normal;
  color: #555;
  width: 100%;
  margin: 0;
  max-width: 100%;
  display: block;
  margin-bottom: 20px;
  background: #fff;
  -webkit-appearance: none;
  -moz-appearance: none;
  -ms-appearance: none;
  -o-appearance: none;
  appearance: none;
}

#k_id_email,
.sign_up {
  margin: 0 0 1em;
  box-sizing: border-box;
  width: 100%;
  line-height: 21px;
}

#k_id_email {
  max-width: 210px;
  border-radius: 0;
  border: none;
  padding: 10px;
  font-size: 16px;
}

.klaviyo_submit_button {
  width: auto;
  padding: 10px;
  border-radius: 0;
  border: none;
}

.klaviyo_field_group,
.klaviyo_form_actions {
  float: left;
}

.success_message {
  font: normal 500 1.25rem / 1.25em "Atlas Grotesk Web", sans-serif;
  text-transform: uppercase;
}

#search {
  display: none;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: 900;
  position: absolute;
  text-align: center;
  overflow: hidden;
  box-sizing: border-box;
  backface-visibility: hidden;
  padding: 40px;
  background: #003658;
}

#search form {
  z-index: 950;
}

#search form input {
  width: 100%;
  box-sizing: border-box;
  border: none;
  border-radius: none;
  padding: 0;
  font-size: 40px;
  background: inherit;
  color: #fff !important;
}

#search .search-close {
  font-size: 32px;
  line-height: 1em;
  position: absolute;
  top: 0;
  right: 0;
  color: #fff;
}

.mobile-search {
  background: inherit;
  width: 100%;
  padding: 10px;
  position: relative;
  top: 0;
  z-index: 2;
  box-sizing: border-box;
}

.mobile-search form {
  margin: 0;
}

.mobile-search input[type=text] {
  border: none;
  border-radius: none;
  font: inherit;
  line-height: 30px;
  outline: none;
  display: block;
  width: 100%;
  height: 30px;
  margin: 0;
  padding: 30px 10px;
  background: #003658;
  color: #fff !important;
  box-sizing: border-box;
}

.relative {
  position: relative;
}

{% comment %} .right {
  float: right !important;
  position: relative;
} {% endcomment %}

.hero {
  position: relative;
  margin-top: 3em;
  margin-bottom: 3em;
}

@media screen and (max-width: 800px) {
  .hero.flexslider {
    margin-bottom: -1.5rem;
  }
}

.hero:first-child {
  margin-top: 0;
}

.hero-caption {
  margin: 0;
  box-sizing: border-box;
  max-width: 50%;
  padding-left: 10%;
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
  text-align: center;
}

@media screen and (max-width: 800px) {
  .hero-caption {
    padding: 0 10px !important;
    -webkit-transform: none;
    -ms-transform: none;
    transform: none;
    position: static;
    max-width: 100%;
    color: inherit !important;
    text-align: center;
    margin-top: 20px;
    margin-bottom: 40px;
  }
}

@media screen and (min-width: 801px) {
  .hero-caption.align-center {
    padding-left: 0;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
  }
}

.hero-caption .headline {
  font: normal 700 1.75rem / 1.25em "Atlas Grotesk Web", sans-serif;
  text-transform: none;
  color: inherit;
}

.hero-caption .subtitle {
  font-size: 1.25em;
  line-height: 1.1em;
  margin-top: 1.75rem;
  margin-bottom: 2.1rem;
}

.hero-caption .action_button {
  color: #16191e;
  border: 3px solid #16191e;
  border-radius: 0;
  padding: 12px 32px;
  background-color: transparent;
  box-shadow: none;
  position: relative;
  font: normal 500 1.25rem / 1.25em "Atlas Grotesk Web", sans-serif;
  text-transform: uppercase;
  margin-bottom: 20px;
}

@media (max-width: 800px) {
  .hero-caption .headline,
  .hero-caption .subtitle,
  .hero-caption .action_button {
    color: inherit !important;
    border-color: inherit !important;
  }
}

.main.content {
  max-width: 100%;
  min-height: 75vh;
  width: 100%;
  overflow: hidden;
  background: #fff;
}

.container {
  margin: 0 auto;
  padding: 0;
  position: relative;
  max-width: 1400px;
}

.page-container {
  box-shadow: 0px -10px 10px rgba(0, 0, 0, 0.5);
  position: relative;
}

.span6,
.span-one-fifth {
  width: 100%;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  float: left;
  margin: auto;
  padding: 0 1rem;
  box-sizing: border-box;
}

@media screen and (min-width: 768px) {
  .span6 {
    max-width: 50%;
  }

  .span-one-fifth {
    max-width: 20%;
  }
}

#header {
  display: none;
  text-align: center;
  background: #fff;
  z-index: 1001;
  width: 100%;
  padding: 0.5em;
  box-sizing: border-box;
}

#header a span {
  display: none;
}

#header .logo {
  padding-top: 5px;
}

#header .icon-menu {
  float: left;
  font-size: 25px;
  padding-top: 2px;
}

#header .icon-cart {
  float: right;
  font-size: 20px;
  padding-top: 4px;
}

.header {
  z-index: 900;
  width: 100%;
  margin: auto;
  position: fixed !important;
  top: 0;
  background: rgba(255, 255, 255, 0.95);
  color: #16191e;
}

.menu-left {
  left: 0;
}

.menu-right {
  right: 0;
}

.logo {
  display: inline-block;
}

.logo a,
.logo img {
  display: block;
}

.cart_count {
  display: inline-block;
  margin-left: 0.25em;
  background-color: #16191e;
  color: #fff;
  line-height: 1.5em;
  text-align: center;
  padding: 0.1em;
  width: 1.25em;
  height: 1.25em;
  border-radius: 50%;
}

@media (max-width: 1200px) {
  .cart_count {
    line-height: 1em;
    width: 1.2em;
    height: 1.2em;
    font-size: 0.9em;
    position: relative;
    top: -2px;
  }
}

.cart_count.cart-empty {
  background-color: transparent;
  color: inherit;
  padding: 0;
}

.top-header {
  background: #f1f1f3;
  height: auto;
  padding: 0.25rem 0;
}

.top-header .menu-left {
  top: 0.2rem;
}

.top-header .menu-right {
  top: 0.1rem;
}

.top-header .banner {
  max-width: calc(100% - 200px);
  margin: auto;
}

.contact-form {
  margin-bottom: 0;
  margin-left: 0px;
}

.featured_links {
  margin-top: 3em;
  margin-bottom: 3em;
  overflow: hidden;
}



.thumbnail {
  position: relative;
  text-align: center;
  margin-bottom: 1.75rem !important;
  display: inline-block;
  float: none;
  margin-right: -0.25rem;
  vertical-align: top;
}

.thumbnail img {
  display: block;
  margin: auto;
  width: 100%;
}

.thumbnail .size-options {
  display: block;
  position: absolute;
  bottom: 0;
  width: 100%;
  background: rgba(0, 0, 0, 0.15);
  color: #fff;
  max-height: 0%;
  overflow: hidden;
}

.thumbnail .size-link {
  padding: 0.5em 0.25em;
  display: inline-block;
  opacity: 0.7;
}

.page-caption > div:first-of-type:after {
  display: block;
  content: '';
  width: 100px;
  margin: 1.5em auto;
  border-top: 3px solid #16191e;
}

.page-subheading {
  text-align: center;
  color: #16191e;
  text-transform: uppercase;
}

.page-heading {
  font-weight: 500;
  text-align: center;
  font-size: 1.3rem;
  line-height: 1.25em;
}

.page-subheading,
.page-heading {
  max-width: 75%;
  margin: 0 auto;
}

.page-caption {
  margin: 3em auto;
  text-align: center;
}

@media (min-width: 768px) {
  #newsletter-popup {
    max-width: 75%;
    max-width: 75vw;
    -webkit-display: flex;
    -moz-display: flex;
    -ms-display: flex;
    display: flex;
  }
}

#newsletter-popup .span6 {
  padding: 0;
}

@media (max-width: 800px) {
  #newsletter-popup .span6 > img {
    display: none;
  }
}

#newsletter-popup .popup-container {
  text-align: center;
  margin: 0 auto;
  max-width: 75%;
}

#newsletter-popup h3 {
  margin-bottom: 1rem;
}

#newsletter-popup p:last-of-type {
  margin: 0;
}

#newsletter-popup #k_id_email {
  background: #e8e9e9;
  width: 100%;
  max-width: 100%;
  text-align: center;
  line-height: 1em;
  vertical-align: middle;
  margin-top: 1.5rem;
  margin-bottom: 1.5rem;
  padding: 1rem;
}

#newsletter-popup .klaviyo_field_group,
#newsletter-popup .klaviyo_form_actions {
  display: block;
  margin: 0;
  float: none;
}

#newsletter-popup .action_button {
  color: #16191e;
  border: 3px solid #16191e;
  border-radius: 0;
  padding: 0.5rem 4rem;
  background-color: transparent;
  box-shadow: none;
  position: relative;
  font: normal 500 1.25rem / 1.25em "Atlas Grotesk Web", sans-serif;
  text-transform: uppercase;
  line-height: 1em;
  vertical-align: middle;
}

#newsletter-popup .klaviyo_messages {
  margin: 1.5rem 0;
}

.slides {
  margin: 0;
  padding: 0;
  list-style: none;
}

ul.slides li {
  border: none;
  padding: 0;
  margin-bottom: 0;
  position: relative;
}

.flexslider {
  padding: 0;
  z-index: 0;
}

.flexslider .slides > li {
  display: none;
  -webkit-backface-visibility: hidden;
}

.flexslider .slides img {
  display: block;
}

.slides:after {
  content: "\0020";
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
}

.flexslider .slides img {
  display: block;
  width: 100%;
}

.flexslider {
  position: relative;
  clear: both;
  overflow: hidden;
}

.flexslider .slides li {
  text-align: center;
  position: relative;
}

.container:after {
  content: "\0020";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

.clearfix:before,
.clearfix:after,
.container:after {
  content: '\0020';
  display: block;
  overflow: hidden;
  visibility: hidden;
  width: 0;
  height: 0;
}

.container:after,
.clearfix:after {
  clear: both;
}

@media only screen and (min-width: 1201px) {
  .main.content {
    padding-top: 108px;
  }
}

@media only screen and (max-width: 1200px) {
  .header {
    background-color: #fff;
  }

  .mobile_hidden {
    display: none !important;
  }

  #header {
    display: block;
  }

  .header {
    position: relative;
    text-align: center;
  }

  .flexslider a.action_button {
    width: initial;
  }

  .sign_up {
    margin-left: 0;
    width: 100%;
  }
}

@media only screen and (min-width: 480px) and (max-width: 1200px) {
  .mobile_hidden {
    display: none !important;
  }
}

@media only screen and (max-width: 479px) {
  .mobile_hidden {
    display: none !important;
  }
}

@media only screen and (max-device-width: 1024px) {
  :root {
    font-size: 12px;
  }
}

